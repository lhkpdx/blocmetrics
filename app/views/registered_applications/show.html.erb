<h3>NAME: <%= @registered_application.name %></h3>
 <div class="row">
   <div class="col-md-8">
     <p class="lead">
       <h4>URL: <%= link_to @registered_application.url %><h4></p>
       <small>Created by: <%= @registered_application.user.email %></small>
   </div>
 </div>
 <%= link_to "add event", "javascript:blocmetrics.report('sale')" %>
 <% if @events %>
   <%= render partial: 'event', locals: { name: @name, created_at: @created_at } %>
 <% end %>
 </div>
 <br><br>
 <div class="container"
    <div class="row">
      <div class="col-md-4">
         <%= link_to "Back", registered_applications_path %> |
         <%= link_to "Home", root_path %> |
         <%= link_to "Edit", edit_registered_application_path %> |
         <%= link_to "Delete", @registered_application, method: :delete, data: { confirm: 'Are you sure you want to delete this application?' } %>
      </div>
    </div>
  </div>
